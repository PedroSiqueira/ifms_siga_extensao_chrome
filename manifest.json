{
   "action": {
      "default_icon": "img/favicon.png"
   },
   "background": {
      "service_worker": "background.js"
   },
   "web_accessible_resources": [
      {
         "resources": [
            "html/modal_alert.html",
            "html/academico/propostaplano.html"
         ],
         "matches": [
            "https://academico.ifms.edu.br/*"
         ]
      }
   ],
   "content_scripts": [
      {
         "js": [
            "js/suapScript.js"
         ],
         "matches": [
            "https://suap.ifms.edu.br/*"
         ]
      },
      {
         "js": [
            "js/academicoPlanoEnsino.js",
            "js/academico.js"
         ],
         "matches": [
            "https://academico.ifms.edu.br/administrativo/professores/diario/*/plano_ensino/editar/*"
         ]
      },
      {
         "js": [
            "js/academicoConteudo.js",
            "js/academico.js"
         ],
         "matches": [
            "https://academico.ifms.edu.br/administrativo/professores/diario/*/conteudo"
         ]
      }
   ],
   "description": "Assistente para preencher diários no acadêmico e imprimir folhas-ponto no suap.",
   "icons": {
      "128": "img/favicon.png"
   },
   "manifest_version": 3,
   "name": "Scripts IFMS",
   "permissions": [
      "declarativeContent",
      "storage"
   ],
   "version": "0.4.1"
}