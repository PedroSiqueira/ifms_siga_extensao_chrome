{
   "action": {
      "default_icon": "img/favicon.png"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [
      {
         "js": [
            "js/suapScript.js"
         ],
         "matches": [
            "https://suap.ifms.edu.br/*"
         ]
      },
      {
         "js": [
            "js/academicoScript.js"
         ],
         "matches": [
            "https://academico.ifms.edu.br/*"
         ]
      }
   ],
   "description": "Assistente para preencher diários no acadêmico e imprimir folhas-ponto no suap.",
   "icons": {
      "128": "img/favicon.png"
   },
   "manifest_version": 3,
   "name": "Scripts IFMS",
   "permissions": [
      "declarativeContent",
      "storage"
   ],
   "version": "0.4.0"
}